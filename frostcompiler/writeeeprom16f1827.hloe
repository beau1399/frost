 GLOBAL writeeeprom

 
writeeepromhloe CODE
 
 
writeeeprom:
 banksel EEADRL
 POP
 movwf	EEDATL
 POP
 movwf	EEADRL
 bcf EECON1, CFGS
 bcf EECON1, EEPGD
 bsf EECON1, WREN
 bcf	INTCON, GIE
 movlw	55h
 movwf	EECON2
 movlw	0AAh
 movwf	EECON2
